<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
 
	<item android:state_pressed="false">
        <shape>
            <solid android:color="#FFC9DAF8"/>
            <stroke android:width="2dp" android:color="#FFC9DAF8"/>
		    <!--corners android:radius="8dp" 弧形的半径 -->
		    
        </shape>
        
    </item>
    
	
	<item android:state_pressed="true">
        <shape>
            <solid android:color="#80C9DAF8"/>
            <stroke android:width="2dp" android:color="#80C9DAF8"/>
		    <!--corners android:radius="8dp" 弧形的半径 -->
        </shape>
    </item>

</selector>
