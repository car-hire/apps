	<item android:state_pressed="false">
        <shape>
            <solid android:color="#a2c4c9"/>
            <stroke android:width="2dp" android:color="#a2c4c9"/>
		    <!-- android:radius 弧形的半径 -->  
		    <corners android:radius="8dp" />
        </shape>
    </item>
    
	
	<item android:state_pressed="true">
        <shape>
            <solid android:color="#6d9eeb"/>
            <stroke android:width="2dp" android:color="#6d9eeb"/>
		    <!-- android:radius 弧形的半径 -->  
		    <corners android:radius="8dp" />
        </shape>
    </item>
     

</selector>
